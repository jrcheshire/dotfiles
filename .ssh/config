Host *
    ControlMaster auto
    ControlPath ~/.ssh/master-%r@%h:%p
    ControlPersist yes
    ServerAliveInterval 15

# Cannon login nodes
Host odyssey login.rc.fas.harvard.edu
    HostName login.rc.fas.harvard.edu
    User jcheshire

# bicep-dev collaboration-owned node
Host holybicep01 bicep-dev holybicep02
    HostName localhost
    User jcheshire
    Port 8081
    ProxyCommand ssh spud -W %h:%p
